{
  "field_rules": {
    "company_info": {
      "company_name": {
        "keywords": ["公司名称", "企业名称", "单位名称", "投标人名称", "投标单位", "供应商名称", "承包人名称", "乙方", "卖方"],
        "patterns": [".*公司.*名称", ".*企业.*名称", ".*单位.*名称"],
        "priority": 1,
        "field_key": "companyName"
      },
      "established_date": {
        "keywords": ["成立日期", "注册日期", "成立时间", "注册时间", "成立年月", "设立日期", "创建日期"],
        "patterns": [".*成立.*日期", ".*注册.*日期", ".*成立.*时间"],
        "priority": 2,
        "field_key": "establishDate"
      },
      "legal_representative": {
        "keywords": ["法定代表人", "法人代表", "法人", "法定代表人姓名", "企业法人", "法人姓名"],
        "patterns": [".*法.*代表人", ".*法人.*"],
        "priority": 1,
        "field_key": "legalRepresentative"
      },
      "registered_capital": {
        "keywords": ["注册资本", "注册资金", "资本金", "注册资本金", "实收资本", "投资总额"],
        "patterns": [".*注册.*资本", ".*注册.*资金"],
        "priority": 2,
        "field_key": "registeredCapital"
      },
      "business_scope": {
        "keywords": ["经营范围", "业务范围", "主营业务", "营业范围", "经营业务", "业务内容"],
        "patterns": [".*经营.*范围", ".*业务.*范围"],
        "priority": 3,
        "field_key": "businessScope"
      },
      "company_address": {
        "keywords": ["公司地址", "企业地址", "注册地址", "地址", "住所", "经营地址", "办公地址", "注册住所"],
        "patterns": [".*公司.*地址", ".*注册.*地址", ".*企业.*地址"],
        "priority": 2,
        "field_key": "companyAddress"
      },
      "contact_phone": {
        "keywords": ["联系电话", "电话", "联系方式", "手机", "电话号码", "联系人电话", "企业电话", "公司电话"],
        "patterns": [".*联系.*电话", ".*电话.*", ".*手机.*"],
        "priority": 2,
        "field_key": "contactPhone"
      },
      "email": {
        "keywords": ["邮箱", "电子邮箱", "邮箱地址", "E-mail", "Email", "电子邮件", "企业邮箱"],
        "patterns": [".*邮箱.*", ".*[Ee]-?mail.*"],
        "priority": 3,
        "field_key": "email"
      },
      "social_credit_code": {
        "keywords": ["统一社会信用代码", "社会信用代码", "信用代码", "组织机构代码", "营业执照号", "营业执照号码", "税务登记号"],
        "patterns": [".*信用代码.*", ".*营业执照.*号"],
        "priority": 1,
        "field_key": "socialCreditCode"
      }
    },
    "financial_info": {
      "bank_name": {
        "keywords": ["开户银行", "开户行", "银行名称", "基本户开户行", "银行", "开户行名称"],
        "patterns": [".*开户.*银行", ".*开户行.*"],
        "priority": 3,
        "field_key": "bankName"
      },
      "bank_account": {
        "keywords": ["银行账号", "账号", "银行账户", "基本账户", "对公账户", "企业账户"],
        "patterns": [".*银行.*账号", ".*账户.*"],
        "priority": 3,
        "field_key": "bankAccount"
      }
    },
    "project_info": {
      "project_name": {
        "keywords": ["项目名称", "工程名称", "标的名称", "采购项目"],
        "patterns": [".*项目.*名称", ".*工程.*名称"],
        "priority": 1,
        "field_key": "projectName"
      },
      "project_budget": {
        "keywords": ["项目预算", "预算金额", "投资额", "项目金额"],
        "patterns": [".*项目.*预算", ".*预算.*金额"],
        "priority": 2,
        "field_key": "projectBudget"
      },
      "project_duration": {
        "keywords": ["项目周期", "工期", "实施周期", "建设周期"],
        "patterns": [".*项目.*周期", ".*工期.*"],
        "priority": 2,
        "field_key": "projectDuration"
      }
    }
  },
  "processing_rules": {
    "date_formats": [
      "%Y-%m-%d",
      "%Y/%m/%d",
      "%Y年%m月%d日",
      "%Y.%m.%d"
    ],
    "output_date_format": "%Y年%m月%d日",
    "currency_formats": {
      "large_amount_threshold": 10000,
      "large_format": "{:.2f}万元",
      "small_format": "{}元"
    },
    "skip_empty_tables": true,
    "min_confidence": 0.6,
    "max_table_size": 100
  },
  "fillable_patterns": [
    "^_+$",
    "^\\.*$",
    "^-+$",
    "^\\s*$",
    "^（\\s*）$",
    "^\\[\\s*\\]$",
    "^【\\s*】$",
    "请填写",
    "请输入",
    "待填",
    "此处填写",
    "必填",
    "选填"
  ]
}